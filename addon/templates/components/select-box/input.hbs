<input
  {{did-insert (action "insertedElement")}}
  {{will-destroy (action "willDestroyElement")}}
  {{on "input" (action "input")}}
  {{on "keydown" (action "keydown")}}
  aria-multiline="false"
  autocomplete="off"
  class={{select-box-class @classNamePrefix "input"}}
  role="searchbox"
  type="text"

  {{! Issue: https://github.com/emberjs/rfcs/issues/497 }}
  aria-controls={{@aria-controls}}
  disabled={{@disabled}}

  ...attributes>