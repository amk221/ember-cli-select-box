<input
  {{did-insert (action "_registerDomElement")}}
  {{will-destroy (action "_deregisterDomElement")}}
  aria-multiline="false"
  autocomplete="off"
  class={{select-box-class @classNamePrefix "input"}}
  id={{this.domElementId}}
  role="searchbox"
  type="text"
  oninput={{action "_oninput"}}
  onkeydown={{action "_onkeydown"}}

  {{! Issue: https://github.com/emberjs/rfcs/issues/497 }}
  aria-controls={{@aria-controls}}
  disabled={{@disabled}}

  ...attributes>