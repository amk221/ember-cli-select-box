<input
  {{did-insert (action "registerElement" this)}}
  {{will-destroy (action "deregisterElement" this)}}
  {{on "input" (action "input" this)}}
  {{on "keydown" (action "keydown" this)}}
  aria-multiline="false"
  autocomplete="off"
  class={{select-box-class @classNamePrefix "input"}}
  role="searchbox"
  type="text"

  {{! Issue: https://github.com/emberjs/rfcs/issues/497 }}
  aria-controls={{@aria-controls}}
  disabled={{@disabled}}

  ...attributes>