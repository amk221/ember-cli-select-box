{{yield (hash
  option=(component "select-box/native/option"
    class-prefix=@class-prefix
    -parent-components=this.options
    -parent-api=this.api
    -parent-is-multiple=this.isMultiple
    -parent-internal-value=this.internalValue
    -on-init=(action "_registerOption")
    -on-destroy=(action "_deregisterOption")
    -on-select=(action "select"))
  group=(component "select-box/native/group"
    class-prefix=@class-prefix)
)}}