{{yield (hash
  selected-options=(component "select-box/selected-options"
    class-prefix=@class-prefix
    aria-activedescendant=this.activeSelectedOption.elementId
    _onInit=(action "_registerSelectedOptionsContainer")
    _onDestroy=(action "_deregisterSelectedOptionsContainer"))
  selected-option=(component "select-box/selected-option"
    class-prefix=@class-prefix
    _parentComponents=this.selectedOptions
    _parentActiveIndex=this.activeSelectedOptionIndex
    _onInit=(action "_registerSelectedOption")
    _onDestroy=(action "_deregisterSelectedOption")
    _onActivate=(action "activateSelectedOptionAtIndex"))
  options=(component "select-box/options"
    class-prefix=@class-prefix
    aria-activedescendant=this.activeOption.elementId
    _onInit=(action "_registerOptionsContainer")
    _onDestroy=(action "_deregisterOptionsContainer"))
  option=(component "select-box/option"
    class-prefix=@class-prefix
    _parentComponents=this.options
    _parentActiveIndex=this.activeOptionIndex
    _parentApi=this.api
    _parentIsMultiple=this.isMultiple
    _parentInternalValue=this.internalValue
    _onInit=(action "_registerOption")
    _onDestroy=(action "_deregisterOption")
    _onActivate=(action "activateOptionAtIndex")
    _onSelect=(action "_select"))
  group=(component "select-box/group"
    class-prefix=@class-prefix)
  input=(component "select-box/input"
    class-prefix=@class-prefix
    autocomplete="off"
    disabled=this.isDisabled
    _parentApi=this.api
    _onInit=(action "_registerInput")
    _onDestroy=(action "_deregisterInput")
    _onInput=(action "_inputText"))
  element=this.element
  value=this.internalValue
  isPending=this.isPending
  isRejected=this.isRejected
  isFulfilled=this.isFulfilled
  isSettled=this.isSettled
  isSearching=this.isSearching
  isSlowSearch=this.isSlowSearch
  isOpen=this.isOpen
  open=(action "open")
  close=(action "close")
  toggle=(action "toggle")
  select=(action "select")
  update=(action "update")
  selectActiveOption=(action "selectActiveOption")
  search=(action "search")
  stopSearching=(action "stopSearching")
  setInputValue=(action "setInputValue")
  focusInput=(action "focusInput")
  blurInput=(action "blurInput")
  activateOptionAtIndex=(action "activateOptionAtIndex")
  activateNextOption=(action "activateNextOption")
  activatePreviousOption=(action "activatePreviousOption")
  deactivateOptions=(action "deactivateOptions")
  activateSelectedOptionAtIndex=(action "activateSelectedOptionAtIndex")
  activateNextSelectedOption=(action "activateNextSelectedOption")
  activatePreviousSelectedOption=(action "activatePreviousSelectedOption")
  deactivateSelectedOptions=(action "deactivateSelectedOptions")
)}}
